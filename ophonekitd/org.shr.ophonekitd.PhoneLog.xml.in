<?xml version="1.0"?>
<node name="/" xmlns:fso="http://www.freesmartphone.org/schemas/DBusSpecExtension" xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd" doc:id="shr-ophonekitd-phonelog-interface">

<doc:doc>
  <doc:summary>SHR ophonekitd PhoneLog Interface</doc:summary>
  <doc:version>0.0.0.1</doc:version>
</doc:doc>

<interface name="org.shr.ophonekitd.PhoneLog">
	<doc:doc>
		<doc:para>
			The PhoneLog Interface is used to show the screen
			with a list of call events (incoming, outgoing,
			missed) optionally filtered by some criteria.
		</doc:para>
	</doc:doc>

	<method name="DisplayList">
		<arg type="a{sv}" name="filter" direction="in">
			<doc:doc>
				<doc:summary>
					List of filter attributes to limit the
					list of calls to be shown.
				</doc:summary>
			</doc:doc>
		</arg>
	</method>
</interface>
</node>


