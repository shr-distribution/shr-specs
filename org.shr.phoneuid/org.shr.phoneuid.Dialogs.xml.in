<?xml version="1.0"?>
<node name="/" xmlns:fso="http://www.freesmartphone.org/schemas/DBusSpecExtension" xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd" doc:id="shr-ophonekitd-dialogs-interface">

<doc:doc>
  <doc:summary>SHR phoneuid Dialogs Interface</doc:summary>
  <doc:version>0.0.0.1</doc:version>
</doc:doc>

<interface name="org.shr.phoneuid.Dialogs">
	<method name="DisplaySimAuth">
		<arg type="i" name="status" direction="in"></arg>
	</method>
	<method name="HideSimAuth">
		<arg type="i" name="status" direction="in"></arg>
	</method>
	<method name="DisplayUssd">
		<arg type="i" name="mode" direction="in"></arg>
		<arg type="s" name="message" direction="in"></arg>
	</method>
	<method name="DisplayDialog">
		<arg type="i" name="dialog" direction="in"></arg>
	</method>
</interface>
</node>

