<?xml version="1.0"?>
<node name="/" xmlns:fso="http://www.freesmartphone.org/schemas/DBusSpecExtension" xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd" doc:id="shr-ophonekitd-callmanagement-interface">

<doc:doc>
  <doc:summary>SHR phoneuid CallManagement Interface</doc:summary>
  <doc:version>0.0.0.1</doc:version>
</doc:doc>

<interface name="org.shr.phoneuid.CallManagement">
	<method name="DisplayIncoming">
		<arg type="i" name="call_id" direction="in"></arg>
		<arg type="i" name="status" direction="in"></arg>
		<arg type="s" name="number" direction="in"></arg>
	</method>
	<method name="HideIncoming">
		<arg type="i" name="call_id" direction="in"></arg>
	</method>
	<method name="DisplayOutgoing">
		<arg type="i" name="call_id" direction="in"></arg>
		<arg type="i" name="status" direction="in"></arg>
		<arg type="s" name="number" direction="in"></arg>
	</method>
	<method name="HideOutgoing">
		<arg type="i" name="call_id" direction="in"></arg>
	</method>
</interface>
</node>

